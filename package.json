{
  "name": "dacc-js",
  "version": "0.0.5",
  "description": "Dacc-js | Decentralized Account Control (DAcc) on Blockchain networks.",
  "license": "GPL-3.0-only",
  "author": "nidz-the-fact",
  "repository": "https://github.com/thefactlab-org/dacc-js",
  "homepage": "https://dacc-js.thefactlab.org",
  "keywords": [
    "dacc",
    "dacc-js",
    "wallet",
    "web3",
    "crypto",
    "encryption",
    "decryption",
    "hashing",
    "password",
    "jwt",
    "eth",
    "ethereum",
    "evm",
    "dapps",
    "typescript"
  ],
  "module": "index.ts",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "dacc": "./dist/bin/dacc.js"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --minify && tsc --outDir ./dist && bun build:cli",
    "build:cli": "bun build ./src/bin/dacc.ts --target node --outdir ./dist/bin --minify --sourcemap=none"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/bun": "1.3.1",
    "@types/libsodium-wrappers-sumo": "^0.7.8",
    "@types/prompts": "^2.4.9"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "libsodium-wrappers-sumo": "^0.7.15",
    "prompts": "^2.4.2",
    "viem": "^2.38.2"
  }
}
